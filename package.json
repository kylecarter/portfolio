    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "build": "yarn run build:node",
    "build:node": "./node_modules/.bin/webpack --mode ${NODE_ENV}",
    "build:python": "python ./backend/manage.py collectstatic --noinput",
    "dev": "yarn run build && yarn run dev:server",
    "dev:server": "concurrently --kill-others \"yarn run dev:server:node\"",
    "dev:server:node": "nodemon --exec babel-node ./bin/www",
    "dev:server:python": "python ./backend/manage.py runserver",
    "lint": "./node_modules/.bin/eslint ./bin --ext .js,.jsx --ignore-path ./bin/backend --fix",
    "postinstall": "yarn run build",
    "start": "concurrently --kill-others \"yarn run start:python\" \"yarn run start:node\"",
    "start:node": "babel-node --optimize_for_size --max_old_space_size=920 --gc_interval=100 --expose-gc ./bin/www",
    "start:python": "gunicorn backend.backend.wsgi:application --bind ${DJANGO_HOST}:${DJANGO_PORT}",
    "test": "yarn run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kylecarter/portfolio.git"
  },
  "keywords": [
    "react",
    "django",
    "javascript",
    "python",
    "html",
    "css",
    "html5",
    "css3"
  ],
  "author": "Kyle A. Carter <kyle.andrew.carter@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kylecarter/portfolio/issues"
  },
  "homepage": "https://github.com/kylecarter/portfolio#readme",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "",
      "pre-push": ""
    }
  }
}

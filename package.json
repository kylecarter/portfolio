{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "This repo represents the personal website for Kyle Carter to act as a portfolio for his professional web development knowledge.",
  "main": "index.js",
  "repository": "https://github.com/kylecarter/portfolio.git",
  "author": "Kyle Carter <kyle.andrew.carter@gmail.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "javascript",
    "html",
    "css",
    "python"
  ],
  "scripts": {
    "build": "run-script-os",
    "build:default": "./node_modules/.bin/webpack --mode $NODE_ENV",
    "build:windows": "./node_modules/.bin/webpack --mode %NODE_ENV%",
    "dev": "node ./frontend/bin/www",
    "lint": "./node_modules/.bin/eslint --ext .js, .vue --fix src",
    "postinstall": "yarn run build",
    "start": "node --optimize_for_size --max_old_space_size=920 --gc_interval=100 --expose-gc ./frontend/bin/www",
    "test": "yarn run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "",
      "pre-push": ""
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "hbs": "^4.1.1",
    "http-errors": "^1.8.0",
    "morgan": "^1.10.0",
    "normalize.css": "^8.0.1",
    "reset.css": "^2.0.2",
    "run-script-os": "^1.1.5",
    "vue": "^2.6.12",
    "vue-router": "^3.5.1",
    "vue-server-renderer": "^2.6.12",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/node": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/register": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "@vue/babel-preset-app": "^4.5.11",
    "autoprefixer": "^10.2.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.5",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.4",
    "postcss-css-variables": "^0.17.0",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.0.0",
    "postcss-url": "^10.1.1",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "vue-loader": "^15.9.6",
    "webpack": "^5.21.1",
    "webpack-cli": "^4.5.0"
  }
